#include "sda670.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. QCS605";
	qcom,msm-id = <347 0x0>;
};

&pil_modem_mem {
	reg = <0 0x8b000000 0 0x3100000>;
};

&pil_video_mem {
	reg = <0 0x8e100000 0 0x500000>;
};

&wlan_msa_mem {
	reg = <0 0x8e600000 0 0x100000>;
};

&pil_cdsp_mem {
	reg = <0 0x8e700000 0 0x800000>;
};

&pil_mba_mem {
	reg = <0 0x8ef00000 0 0x200000>;
};

&pil_adsp_mem {
	reg = <0 0x8f100000 0 0x1e00000>;
};

&pil_ipa_fw_mem {
	reg = <0 0x90f00000 0 0x10000>;
};

&pil_ipa_gsi_mem {
	reg = <0 0x90f10000 0 0x5000>;
};

&pil_gpu_mem {
	reg = <0 0x90f15000 0 0x2000>;
};

&adsp_mem {
	size = <0 0x800000>;
};

&secure_display_memory {
	status = "disabled";
};

&sp_mem {
	status = "disabled";
};

&dump_mem {
	size = <0 0x800000>;
};

&soc {
	qcom,rmnet-ipa {
		status = "disabled";
	};
};

&ipa_hw {
	status = "disabled";
};

&mem_dump {
	rpmh {
		qcom,dump-size = <0x400000>;
	};
};

&thermal_zones {
	lmh-dcvs-00 {
		trips {
			active-config {
				temperature = <105000>;
				hysteresis = <40000>;
			};
		};
	};

	lmh-dcvs-01 {
		trips {
			active-config {
				temperature = <105000>;
				hysteresis = <40000>;
			};
		};
	};
};

&msm_gpu {
	/delete-node/qcom,gpu-mempools;
};
