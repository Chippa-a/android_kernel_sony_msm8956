&clock_gcc {
	compatible = "qcom,gcc-8976-v1";
};

&clock_gcc_mdss {
	compatible = "qcom,gcc-mdss-8976-v1";
};

&soc {
	/delete-node/ qcom,cpu-clock-8976@b016000;

	clock_cpu: qcom,cpu-clock-8976@b016000 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "qcom,cpu-clock-8976";
		reg =   <0xb114000  0x68>,
			<0xb014000  0x68>,
			<0xb116000  0x40>,
			<0xb016000  0x40>,
			<0xb1d0000  0x40>,
			<0xb111050  0x08>,
			<0xb011050  0x08>,
			<0xb1d1050  0x08>,
			<0x00a412c  0x08>;
