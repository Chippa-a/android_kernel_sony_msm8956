&mdss_dsi {
	/delete-property/ qcom,split-dsi-independent-pll;
};

&mdss_dsi0_pll {
	qcom,ssc-enable;
	qcom,ssc-frequency-hz = <30000>;
	qcom,ssc-ppm = <5000>;
	qcom,ssc-spread-mode = "down";
};

&mdss_dsi1_pll {
	qcom,ssc-enable;
	qcom,ssc-frequency-hz = <30000>;
	qcom,ssc-ppm = <5000>;
	qcom,ssc-spread-mode = "down";
};

