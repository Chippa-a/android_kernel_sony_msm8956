&ufsphy_mem {
	compatible = "qcom,ufs-phy-qrbtc-msmskunk";
	status = "ok";
};

&ufs_mem {
	lanes-per-direction = <1>;
	limit-tx-hs-gear = <1>;
	limit-rx-hs-gear = <1>;
	scsi-cmd-timeout = <300000>;

	qcom,disable-lpm;
	rpm-level = <0>;
	spm-level = <0>;
	status = "ok";
};
