/dts-v1/;

#include "apq8053-lite-dragon-v2.1.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. APQ8053 Lite DragonBoard V2.1";
	compatible = "qcom,apq8053-lite-dragonboard", "qcom,apq8053",
			"qcom,dragonboard";
	qcom,board-id= <0x01010020 0>;
};

&blsp2_uart0 {
	status = "okay";
};

&i2c_3 {
	status = "okay";
	elan_ktf@10 {
		compatible = "elan,ekth3500";
		reg = <0x10>;
		vdd-supply = <&pm8953_l10>;
		vccio-supply = <&pm8953_l6>;
		interrupt-parent = <&tlmm>;
		reset-gpio = <&tlmm 64 GPIO_ACTIVE_LOW>;
		elan,irq-gpio = <&tlmm 65 0x2008>;
		interrupts = <65 0x2>;
	};
};
